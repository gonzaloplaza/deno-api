{
  "deno.enablePaths": ["./"],
  "deno.enable": true,
  "deno.unstable": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.inlayHints.enabled": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "deno.suggest.imports.hosts": {
    "https://deno.land": true
  }
}
